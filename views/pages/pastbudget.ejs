<div class="mainbox">
    <h2 id="heading">Buddy Trackers Dashboard</h2>
    <div class="filters">
        <span>
            <label for="option1">Year</label>
            <select name="year" id="option1" required>
                <option value="" disabled selected>choose your year</option>
                <% for(let i=userLogged.startyear;i<=userLogged.endyear;i++){%>
                    <option value="<%= i%>"><%= i%></option>
                <%}%>
            </select>
        </span>
        <span>
            <label for="option2">Month</label>
            <select name="month" id="option2" required>
                <option value="" disabled selected>choose your Month</option>
                <option value="0">January</option>
                <option value="1">February</option>
                <option value="2">March</option>
                <option value="3">April</option>
                <option value="4">May</option>
                <option value="5">June</option>
                <option value="6">July</option>
                <option value="7">August</option>
                <option value="8">September</option>
                <option value="9">October</option>
                <option value="10">November</option>
                <option value="11">December</option>
            </select>
        </span>
    </div>
    <div class="cards">
        
        </div>
    </div>
</div>
